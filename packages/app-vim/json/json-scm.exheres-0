require vim-plugin [ vim_version="7.3" ]
require github [ user=elzr pn="vim-${PN}" ]

SUMMARY="A better JSON for Vim: distinct highlighting of keywords vs values, JSON-specific (non-JS)
warnings, quote concealing."

LICENCES="MIT"
SLOT="0"
PLATFORMS="~amd64"

src_prepare() {
    # remove unnecessary files
    edo find -maxdepth 1 -type f -exec rm {} \;

    default
}

